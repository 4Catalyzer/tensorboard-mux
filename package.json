{
  "name": "tensorboard-mux",
  "version": "0.0.0",
  "description": "TensorBoard multiplexer",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "bin": {
    "tensorboard-mux": "lib/index.js"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib",
    "lint": "eslint src tools",
    "prepublish": "cross-env BABEL_ENV=dist npm run build",
